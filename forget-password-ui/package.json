{
  "name": "@delivery/forget-password-ui",
  "version": "1.0.4-RC",
  "license": "UNLICENSED",
  "author": "Saint-Gobain DSI Group - Digital Solutions",
  "scripts":   {
    "build": "yarn run env -s && ng build",
    "build:uat": "yarn run env -s && ng build --prod --configuration=uat --build-optimizer --vendor-chunk --common-chunk",
    "build:prod": "yarn run env -s && ng build --prod --build-optimizer --vendor-chunk --common-chunk",
    "build:ci": "yarn test:ci && yarn reports:doc && yarn reports:wp",
    "start": "npm-run-all -p -r start:local start:mock",
    "start:local": "ng serve --proxy-config proxy.conf.js",
    "start:server": "ng serve --proxy-config proxy.server.conf.js --configuration=server",
    "start:mock": "ts-node ./mocking-server/index.ts",
    "lint": "ng lint --type-check --fix && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "lint:fix": "ng lint --fix --force ",
    "test": "ng test --browsers=ChromeNoSandbox --code-coverage",
    "test:ci": "yarn run lint -s && ng test --browsers=ChromeNoSandbox --watch=false --code-coverage",
    "push": "yarn prettify && yarn lint",
    "e2e": "ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "prettify": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "env": "ngx-scripts env npm_package_version",
    "fix-memory-limit": "cross-env LIMIT=2048 increase-memory-limit",
    "generate": "ng generate",
    "reports:doc": "compodoc -p src/tsconfig.app.json -d reports/documentation/",
    "reports:wp": "yarn run build:prod --stats-json && webpack-bundle-analyzer dist/stats.json dist/ -m static -r reports/bundles/index.html",
    "reports:open": "node -e \"require('opn')('./reports/index.html')\"",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner scan -X -Dsonar.host.url=http://sonarqube:9000"
  },
  "dependencies":   {
    "@angular/animations": "^7.1.4",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "browser-detect": "^0.2.28",
    "core-js": "^2.5.7",
    "ng-recaptcha": "^4.2.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies":   {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@compodoc/compodoc": "1.0.9",
    "@ngx-rocket/scripts": "^2.2.0",
    "@types/jasmine": "^3.3.5",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/pako": "^1.0.0",
    "codelyzer": "^4.5.0",
    "cross-env": "^5.2.0",
    "fs-finder": "^1.8.1",
    "htmlhint": "^0.10.1",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-concat": "^0.0.1",
    "json-server": "^0.14.2",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "^0.0.32",
    "npm-run-all": "^4.1.5",
    "parent-module": "^1.0.0",
    "prettier": "^1.15.3",
    "protractor": "^5.4.2",
    "puppeteer": "^1.11.0",
    "require-glob": "^3.2.0",
    "sonar-scanner": "^3.1.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.1.6",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "publishConfig": {"registry": "https://bishop.digital-solutions.saint-gobain.com/repository/npm-internal/"}
}