{
  "groupId": "fr.csn.caf",
  "name": "frontend-caf",
  "version": "0.0.16-SNAPSHOT",
  "dockerImage": "registry.sedona.fr/csn/caf/frontend",
  "description": "[CSN] CAF - Frontend",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --crossOrigin=use-credentials --source-map=false --base-href=/",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint frontend --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:prepush": "npm run lint && npm run lint:styles",
    "lint:ci": "tslint -t json --force -o reports/lint.json --project .",
    "e2e": "ng e2e",
    "compodoc": "npm run coverage && npm run compodoc:coverage",
    "compodoc:coverage": "compodoc -p src/tsconfig.app.json --unitTestCoverage reports/coverage/coverage-summary.json",
    "coverage": "ng test --code-coverage --browsers=ChromeHeadlessNoSandbox --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@ngx-translate/core": "^12.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-charts": "^13.0.2",
    "bootstrap": "4.4.1",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.4",
    "file-saver": "^2.0.2",
    "jquery": "^3.4.1",
    "ng-click-outside": "^5.3.0",
    "ng2-charts": "^2.3.0",
    "ng7-bootstrap-breadcrumb": "^2.0.0",
    "ngx-bootstrap": "^5.3.2",
    "ngx-cookie-service": "^2.3.0",
    "ngx-device-detector": "^1.3.20",
    "ngx-mask": "^8.1.7",
    "ngx-spinner": "~8.1.0",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@compodoc/compodoc": "1.1.11",
    "@schematics/angular": "~9.1.0",
    "@types/jasmine": "~3.5.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.7.0",
    "codelyzer": "^5.2.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "protractor": "~5.4.3",
    "rimraf": "3.0.1",
    "stylelint": "13.0.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:prepush"
    }
  }
}
